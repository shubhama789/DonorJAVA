create table Donor_Details(
Donor_Id varchar2(20),
Donor_Name varchar2(20),
Address varchar2(20),
Phone_Number varchar2(20),
Donor_Date DATE,
Donor_Amount NUMBER);


create sequence donorid_sequence
start with 1000;


insert into donor_details values(donorid_sequence.nexval,?,?,?,SYSDATE,?);

